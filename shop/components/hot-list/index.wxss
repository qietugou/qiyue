@import '../../wxss/sleeve.wxss';

.container {
  display: flex;
  flex-direction: column;
  align-items: center;
  padding-top: 40rpx;
  background-color: #a78686;
}
.inner-container {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  width: 100%;
  margin-top: 32rpx;
}
.right-container {
  display: flex;
  flex-direction: column;
  justify-content: space-between;
}
.title {
  width: 690rpx;
  height: 90rpx;
}
.left {
  display: flex;
  width: 346rpx;
  height: 522rpx;
}
.right {
  display: flex;
  width: 400rpx;
  height: 260rpx;
}
